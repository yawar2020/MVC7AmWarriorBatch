@model List<Ado.net_approach.Models.EmployeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>Empid</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
        <th>Action</th>
    </tr>
    
        @foreach (var item in Model)
        {
           <tr>
               <td>@item.EmpId</td>
               <td>@item.EmpName</td>
               <td>@item.EmpSalary</td>
               <td>
                   @Html.ActionLink("Edit", "Edit", new {id=@item.EmpId })||
                   @Html.ActionLink("Delete", "Delete", new { id = @item.EmpId })
               </td>
           </tr>
        }
   
</table>